/**index.wxss**/
@import "/pages/template/indexPopup.wxss";

/*头部部分 */
.header {
  
  font-size: 20px;
  margin-top: 52px;
  padding-left: 10px;
  font-family: Microsoft JhengHei;
  font-weight: 600;
  color:#3e3939;
  
}
/*横线的部分*/
.line{
 
  width:100%;
  height:3rpx;
  background:#DADADA;
  margin-top: 14px;
}

/*特别推荐轮播图整体属性  */

.body {
  padding: 0px 50rpx;
}

/*特别推荐属性  */

.title0 {
  color: #000;
  font-size: 40rpx;
  font-weight: 900;
  margin-top: 34rpx;
  display: block;
}


/*自媒体联动小程序属性  */


.title2 {
  color: #d4d2d4;
  font-size: 30rpx;
  display: block;
  padding-top: 10rpx;
}

/*轮播图属性  */

.imgsilder {
  border-radius: 25rpx;
  width: 100%;
  height: 328rpx;
  margin: 0rpx auto;
  display: block;
  /* margin-top: 15rpx; *//* border-radius: 25rpx;  */
  overflow: hidden;
  margin-top: 28rpx;
}

.slide-image {
  width: 100%;
  height: 100%;
  border-radius: 25rpx;
}

/*今日联动整体属性  */

.bodytext {
  padding: 0px 50rpx;
  margin-top: 50rpx;
}

/* 今日联动属性   */

.title0 {
  color: #000;
  font-size: 40rpx;
  font-weight: 900;
  margin-top: 34rpx;
  display: block;
}

/*膨胀小人物属性  */

.title3 {
  color: #3e3939;
  font-size: 35rpx;
  display: block;
  margin-top: 37rpx;
  font-weight: 900;
}
/*充电桩情况*/
.battery{
  width: 100%;
  height: 1000rpx;
  background-color:#F8F8F8;
  border: 1px solid transparent;
  padding-top: 40rpx;
  
}

/*搜索结果模块*/

.charge {
  height: 200rpx;
  width: 90%;
  margin: 0 auto;
  background-color:white;
  border-radius: 20rpx;
  margin-bottom: 40rpx;
}

.where {
  padding-left: 16px;
  line-height:50px;
  font-size: 19px;
  font-weight: 550;
  font-family:  Microsoft JhengHei;
  color:rgb(100, 92, 92);
  
}
/*充电桩状态*/
.box{
  width: 80px;
  height: 30px;
  display: inline-block; 
  background-color: #2C9EF4;
  line-height:30px; 
  text-align: center;
  font-size: 17px;
  font-weight: 550;
  font-family:  Microsoft JhengHei;
  color:#FFFFFF;
  border-radius: 30px;
  
  
}

.box1{
  width: 133px;
  height: 30px;
  display: inline-block;
  background-color: #E4F1F9;
  margin-left: 30px ;
  line-height:30px; 
  text-align: center;
  font-size: 18px;
  font-weight: 550;
  font-family:  Microsoft JhengHei;
  color:#2C9EF4;
  border-radius: 30px;
  margin-top: 18px;
}




